<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleView">
        <attr name="strokeWidth" format="dimension"/>
        <attr name="strokeColor" format="color|reference"/>
        <attr name="fillColor" format="color|reference"/>
        <attr name="circleRadius" format="dimension"/>
        <attr name="circleGap" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="JogView">
        <attr name="jogDotOffset" format="dimension"/>
        <attr name="jogDotFillColor" format="color|reference"/>
        <attr name="jogFillColor" format="color|reference"/>
        <attr name="jogStroke" format="dimension"/>
        <attr name="jogRadius" format="dimension"/>
        <attr name="jogDotRadius" format="dimension"/>
        <attr name="jogAngle" format="integer"/>
    </declare-styleable>

</resources>